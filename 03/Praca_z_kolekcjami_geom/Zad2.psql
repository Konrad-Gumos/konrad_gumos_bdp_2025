WITH 
    shortest_line 
    AS (
        SELECT 
            ST_ShortestLine(obiekt3.geometry, obiekt4.geometry) AS geom
        FROM 
            obiekty obiekt3, 
            obiekty obiekt4
        WHERE 
            obiekt3.nazwa = 'obiekt3' AND obiekt4.nazwa = 'obiekt4'
    )
SELECT 
    ST_Area(ST_Buffer(geom, 5))
FROM 
    shortest_line;